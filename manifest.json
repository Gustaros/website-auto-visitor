{
  "name": "Daily Auto Visitor",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "alarms",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}